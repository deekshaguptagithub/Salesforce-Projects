    <validationRules>
        <fullName>Apt_UK__DOBHHAccountCannotBeInFuture</fullName>
        <active>true</active>
        <description>Date of Birth for a Household Account cannot be in the future</description>
        <errorConditionFormula>RecordType.DeveloperName = &apos;HouseholdAccount&apos;  &amp;&amp;  Apt_UK__DateofBirth__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Apt_UK__DateofBirth__c</errorDisplayField>
        <errorMessage>Date of Birth cannot be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apt_UK__DWPOfficeValidation</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
OR(ISPICKVAL(Apt_UK__DWPStatus__c ,&quot;Customer Application&quot;),
ISPICKVAL(Apt_UK__DWPStatus__c ,&quot;Client Application&quot;),
ISPICKVAL(Apt_UK__DWPStatus__c ,&quot;Declined&quot;),
ISPICKVAL(Apt_UK__DWPStatus__c ,&quot;Terminated&quot;),
ISPICKVAL(Apt_UK__DWPStatus__c ,&quot;Active&quot;)),
ISPICKVAL( Apt_UK__DWPOffice__c,&quot;&quot; ) )</errorConditionFormula>
        <errorMessage>DWP Office field needs to be updated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apt_UK__EmailAddressMandatory</fullName>
        <active>true</active>
        <description>Email Address is required if Billing Email Opt-In is selected</description>
        <errorConditionFormula>Apt_UK__BillingEmailOptIn__c = TRUE &amp;&amp; ISBLANK(Apt_UK__Email__c )</errorConditionFormula>
        <errorDisplayField>Apt_UK__Email__c</errorDisplayField>
        <errorMessage>Email Address is required if Billing Email Opt-In is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apt_UK__KeyAccountManagerCannotbeBlank</fullName>
        <active>true</active>
        <description>Key Account Manager cannot be blank if it is a Key Account.</description>
        <errorConditionFormula>Apt_UK__KeyAccount__c = TRUE &amp;&amp; ISBLANK(Apt_UK__KeyAccountManager__c  )</errorConditionFormula>
        <errorDisplayField>Apt_UK__KeyAccountManager__c</errorDisplayField>
        <errorMessage>Key Account Manager cannot be blank if it is a Key Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apt_UK__LOAStartDateCanNotBeAfterLOAEndDate</fullName>
        <active>true</active>
        <description>If LOA End Date is populated then “LOA Start Date cannot be blank” and LOA Start Date must be before End Date or LOA End Date cannot be before Start Date.</description>
        <errorConditionFormula>OR(AND(NOT(ISBLANK(Apt_UK__LOAEndDate__c)),ISBLANK(Apt_UK__LOAStartDate__c)),(Apt_UK__LOAStartDate__c&gt;Apt_UK__LOAEndDate__c ))</errorConditionFormula>
        <errorMessage>If LOA End Date is populated then “LOA Start Date cannot be blank” and LOA Start Date must be before LOA End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apt_UK__NINumberCannotBeblank</fullName>
        <active>true</active>
        <description>This rule throws error when NI Number field is blank and DWP Status is Active.</description>
        <errorConditionFormula>IF( ISPICKVAL(Apt_UK__DWPStatus__c , &quot;Active&quot;), ISBLANK( Apt_UK__NINumber__c ),False )</errorConditionFormula>
        <errorDisplayField>Apt_UK__NINumber__c</errorDisplayField>
        <errorMessage>NI Number can&apos;t be blank when DWP Status is Active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apt_UK__NINumberFormatValidation</fullName>
        <active>true</active>
        <description>This rule is to check the format of the &quot;NI Number&quot; field.</description>
        <errorConditionFormula>IF(NOT(ISBLANK(Apt_UK__NINumber__c)),OR(
NOT(REGEX( Apt_UK__NINumber__c , &quot;[ABCEGHJKLMNOPRSTWXYZ]{1}[ABCEGHJKLMNPRSTWXYZ]{1}[0-9]{2}[0-9]{2}[0-9]{2}[ABCD]{1}&quot;)),
LEFT(Apt_UK__NINumber__c,2) = &quot;BG&quot;,
LEFT(Apt_UK__NINumber__c,2) = &quot;GB&quot;,
LEFT(Apt_UK__NINumber__c,2) = &quot;KN&quot;,
LEFT(Apt_UK__NINumber__c,2) = &quot;NK&quot;,
LEFT(Apt_UK__NINumber__c,2) = &quot;NT&quot;,
LEFT(Apt_UK__NINumber__c,2) = &quot;TN&quot;,
LEFT(Apt_UK__NINumber__c,2) = &quot;ZZ&quot;
),False)</errorConditionFormula>
        <errorDisplayField>Apt_UK__NINumber__c</errorDisplayField>
        <errorMessage>Enter the valid format as &apos;AA111111A&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apt_UK__RecordTypeCannotChange</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNEW()), IsChanged( RecordTypeId ) , OR($Profile.Name = &apos;Client Agent&apos; , $Profile.Name =&apos;Client System Admin&apos;))</errorConditionFormula>
        <errorMessage>Record Type Can&apos;t be updated</errorMessage>
    </validationRules>
