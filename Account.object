        <description>validates if 3rd party is true phone  number must be present</description>
        <errorConditionFormula>AND(  X3rd_Party_Authorisation__c  = TRUE, ISBLANK(  Third_Party_No1__c  ))</errorConditionFormula>
        <errorDisplayField>Third_Party_No1__c</errorDisplayField>
        <errorMessage>Third Party phone must be populated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Third_party_authorisation_type</fullName>
        <active>true</active>
        <description>Validates that an authorisation type must be selected in order to save</description>
        <errorConditionFormula>AND ( X3rd_Party_Authorisation__c = True,  ISPICKVAL( Authorisation_Type1__c , &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Authorisation_Type1__c</errorDisplayField>
        <errorMessage>Authorisation type must be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UKPostalCodeCheck</fullName>
        <active>true</active>
        <description>Postal Code for UK fromat</description>
        <errorConditionFormula>NOT(

				OR(

								ISBLANK( BillingPostalCode ),

								REGEX( BillingPostalCode , &quot;[a-zA-Z]\\d\\s\\d[a-zA-Z]{2}&quot;),

								REGEX( BillingPostalCode , &quot;[a-zA-Z]\\d\\d\\s\\d[a-zA-Z]{2}&quot;),

								REGEX( BillingPostalCode , &quot;[a-zA-Z]{2}\\d\\s\\d[a-zA-Z]{2}&quot;),

								REGEX( BillingPostalCode , &quot;[a-zA-Z]{2}\\d\\d\\s\\d[a-zA-Z]{2}&quot;),

								REGEX( BillingPostalCode , &quot;[a-zA-Z]{2}\\d[a-zA-Z]\\s\\d[a-zA-Z]{2}&quot;),

								REGEX( BillingPostalCode , &quot;[a-zA-Z]\\d[a-zA-Z]\\s\\d[a-zA-Z]{2}&quot;)

				)

)</errorConditionFormula>
        <errorDisplayField>BillingPostalCode</errorDisplayField>
        <errorMessage>Please enter the correct format of Postal Code.</errorMessage>
    </validationRules>
